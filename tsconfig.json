{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "include": ["**/*", "node_modules/nativewind"],
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@assets/*": ["./src/assets/*"],
      "@api/*": ["./src/api/*"],
      "@components/*": ["./src/components/*"],
      "@screens/*": ["./src/screens/*"],
      "@stackNav/*": ["./src/stackNav/*"],
      "@tabNav/*": ["./src/tabNav/*"]
    }
  }
}
